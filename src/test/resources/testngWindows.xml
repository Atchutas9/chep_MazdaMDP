    <listeners>
        <listener class-name="au.com.mazda.MDP.Utility.Listener" />

    </listeners>

    
    <test name="WindowsFirefoxTest">
        <parameter name="browser" value="Windows - firefox" />

       <classes>
            <class name="au.com.mazda.MDP.TestCases.FindAllLinks_Tc">
                <methods>
                    <exclude name="testVerifyLinksCampaigns" />
                </methods>
            </class>

             <class name="au.com.mazda.MDP.TestCases.MenuTc">
                <methods>
                    <include name="testVerifyMenu"/>
                </methods>
            </class>

            <class name="au.com.mazda.MDP.TestCases.PostCodePopUp_Tc">
                <methods>
                    <exclude name="testVerifyPostCodePopupPresence" />
                    <exclude name="testVerifyClosePostCodePopUp" />
                    <exclude name="testVerifySubmitPostCodepopUp" />
                    <exclude name="testVerifyGetCookiePostCode" />
                </methods>
            </class>
            <class name="au.com.mazda.MDP.TestCases.GalleryGrid_Tc">
                <methods>
                    <exclude name="testVerifyGalleryGridPresence" />
                    <exclude name="testVerifyImagesPresence" />
                    <exclude name="testVerifyVideosPresence" />
                </methods>
            </class>
        </classes>
    </test>
